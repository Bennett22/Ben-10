getgenv().Energy = true;

while Wait() do
    if getgenv().Energy == true then
local args = {
    [1] = "UpdateRingPower",
    [2] = 250
}

game:GetService("Players").LocalPlayer.Character:FindFirstChild("Willpower Ring").ActivatedRemote:FireServer(unpack(args))

    end
    end
end)

