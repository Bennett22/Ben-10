        [2] = {
            [1] = "\4",
            [2] = game:GetService("Players").LocalPlayer,
            [3] = game:GetService("SoundService"):WaitForChild("NormalCast")
        },
        [3] = {
            [1] = "\4",
            [2] = game:GetService("Players").LocalPlayer,
            [3] = game:GetService("SoundService"):WaitForChild("NormalCast")
        }
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer(unpack(args))


             end
            end
         end)
	end    

