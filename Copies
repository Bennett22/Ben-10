local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Bennett Scripts | Marvel Enhanced/New Journey Copies", "Serpent")

local Bennett = Window:NewTab("Scripts")
