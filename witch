	Name = "Holy Fire Someone",
	Default = "input",
	TextDisappear = true,
	Callback = function(txt)
      local Players = game:GetService("Players")
      local msg = txt
      local function findPlayer(stringg)
          for _, v in pairs(Players:GetPlayers()) do
