local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Bennett Scripts | Guardians Prophecy", HidePremium = false, IntroText = "Bennett Scripts", SaveConfig = true, ConfigFolder = "OrionTest"})

local BennettTab = Window:MakeTab({
	Name = "Jewels",
	Icon = "rbxassetid://14458282849",
	PremiumOnly = false
})

local Section = BennettTab:AddSection({
	Name = "Mayura"
})

BennettTab:AddButton({
	Name = "Transform to Mayura",
	Callback = function()

local args = {
    [1] = "mayura"
}

game:GetService("ReplicatedStorage"):WaitForChild("transform"):FireServer(unpack(args))

  	end    
})

BennettTab:AddButton({
	Name = "Detransform",
	Callback = function()

local args = {
    [1] = "mayura"
}

game:GetService("ReplicatedStorage"):WaitForChild("detransform"):FireServer(unpack(args))

  	end    
})

BennettTab:AddButton({
	Name = "Delete your powers (click if u want to change jewel)",
	Callback = function()

game.Players.LocalPlayer.Backpack.activatecombat:destroy()
game.Players.LocalPlayer.Backpack.amokatizer:destroy()
game.Players.LocalPlayer.Backpack.clickcomb:destroy()
game.Players.LocalPlayer.Backpack.deamokatizer:destroy()

  	end    
})

BennettTab:AddButton({
	Name = "Show Mayura Gui (if u changed jewel)",
	Callback = function()

game.Players.LocalPlayer.PlayerGui.PlrGui.mayura.Visible = true

  	end    
})

BennettTab:AddButton({
	Name = "Hide Mayura Gui (click if u want to change jewel)",
	Callback = function()

game.Players.LocalPlayer.PlayerGui.PlrGui.mayura.Visible = false

  	end    
})

BennettTab:AddParagraph("Delete Powers","delete them if u want to change jewel")

BennettTab:AddButton({
	Name = "Delete Power 1",
	Callback = function()

game.Players.LocalPlayer.Backpack.amokatizer:destroy()

  	end    
})

BennettTab:AddButton({
	Name = "Delete Power 2",
	Callback = function()

