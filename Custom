BennettSection:NewTextBox("Blood Boil", "", function(txt)
    local Players = game:GetService("Players")
    local msg = txt
    local function findPlayer(stringg)
        for _, v in pairs(Players:GetPlayers()) do
            if stringg:lower() == (v.Name:lower()):sub(1, #stringg) then
                return v
            end
        end
    end

    local player = findPlayer(msg)
    local CHAR = player.Character

local args = {
    [1] = "BloodBoil",
    [2] = "Blood Boil",
    [3] = player.Character,
    [4] = Vector3.new(701.055908203125, 41.5108757019043, 71.009765625)
}

game:GetService("ReplicatedStorage"):WaitForChild("Character_Remotes"):WaitForChild("Rikki"):FireServer(unpack(args))

end)

