BennettSection:NewToggle("Throw Water Orb", "Keybind (U)", function()

    clicktpon = true                
        plr = game.Players.LocalPlayer
        hum = plr.Character.HumanoidRootPart
        mouse = plr:GetMouse()
        mouse.KeyDown:connect(function(key)
        if key == 'u' then
        if mouse.Target then
        if clicktpon then

local args = {
    [1] = "Water",
    [2] = "Water Orb",
    [4] = Vector3.new(mouse.Hit.x, mouse.Hit.y + 2, mouse.Hit.z),
    [5] = "Click"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Cleo"):FireServer(unpack(args))

end
end
end
end)
end)

