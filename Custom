BennettSection:NewButton("Summon Expression", " ", function()
if game:GetService("Players").LocalPlayer.UserId == 4831686579 then
local args = {
    [1] = "I Have Every Magic!",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

wait(2)

local args = {
    [1] = "I Have The Spirits!",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

wait(2)

local args = {
    [1] = "Phasmatos Incendia",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

    local Plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 100000 then
                local Target = Player.Character or Player.CharacterAdded:Wait()

local args = {
    [1] = "Fire",
    [2] = "Fire",
    [3] = Target,
    [4] = Vector3.new(348.40545654296875, -7.884069442749023, 1137.759521484375)
}

game:GetService("ReplicatedStorage"):WaitForChild("Character_Remotes"):WaitForChild("Aquata"):FireServer(unpack(args))

            end
        end
end   

