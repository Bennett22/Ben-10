local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Bennett Scripts | H2O: Mako Magic", "GrapeTheme")

local Bennett = Window:NewTab("Characters")
local BennettSection = Bennett:NewSection("Half Mermaid")

BennettSection:NewButton("Siren Sirena", " ", function()

local args = {
    [1] = "Outfit2",
    [2] = "Siren Sirena"
}

game:GetService("ReplicatedStorage"):WaitForChild("System_Remotes"):WaitForChild("CharacterChoose"):FireServer(unpack(args))

end)

BennettSection:NewButton("Weilan", " ", function()

local args = {
    [1] = "Outfit2",
    [2] = "Weilan"
}

game:GetService("ReplicatedStorage"):WaitForChild("System_Remotes"):WaitForChild("CharacterChoose"):FireServer(unpack(args))

end)

