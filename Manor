    clicktpon = true                
        plr = game.Players.LocalPlayer
        hum = plr.Character.HumanoidRootPart
        mouse = plr:GetMouse()
        mouse.KeyDown:connect(function(key)
        if key == 'x' then
        if mouse.Target then
        if clicktpon then

local args = {
    [1] = "whirl",
    [2] = Vector3.new(mouse.Hit.x, mouse.Hit.y + 0.1, mouse.Hit.z)
}

game:GetService("ReplicatedStorage"):WaitForChild("Demons"):WaitForChild("UpperLevel"):WaitForChild("Shax"):WaitForChild("PowerEvents"):WaitForChild("ShaxEvents"):FireServer(unpack(args))

end
end
end
end)
end)

BennettSection:NewButton("Stop Whirlwind", " ", function()

local args = {
    [1] = "stop",
    [2] = Vector3.new(-425.8410949707031, 7372.71044921875, 951.73828125)
}

