local args = {
    [1] = "attack",
    [2] = Vector3.new(-42.67594909667969, 76.22956848144531, -191.8939208984375),
    [3] = workspace:WaitForChild("Part")
}

game:GetService("ReplicatedStorage"):WaitForChild("Witches"):WaitForChild("GoodWitches"):WaitForChild("Phoebe"):WaitForChild("PowerEvents"):WaitForChild("PhoebeEvents"):FireServer(unpack(args))

