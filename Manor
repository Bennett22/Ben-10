local BennettTab = Window:MakeTab({
	Name = "Other Powers",
	Icon = "rbxassetid://14458282849",
	PremiumOnly = false
})

BennettTab:AddParagraph("Be Queenie or Billie to be able use them as any character!","click divination once to not glitch it! (Divination = G, Life Draining = C)")

BennettTab:AddButton({
	Name = "Get Divination",
	Callback = function()

    local plr = game.Players.LocalPlayer

local backup = game.Workspace:FindFirstChild("Divination",true):Clone()
backup.Parent = game.Workspace

function give()
   local c = game.Workspace.Divination:Clone()
   c.Parent = game.workspace[plr.name]
end

give()

plr.CharacterAdded:Connect(function()
   give()
end)

  	end    
})

