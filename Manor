local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Bennett Scripts | [MANOR] Charmed", "GrapeTheme")

local Bennett = Window:NewTab("Powers")
local BennettSection = Bennett:NewSection("Powers Section")

BennettSection:NewToggle("Lightning Strike", "Keybind (C)", function()

    clicktpon = true                
        plr = game.Players.LocalPlayer
        hum = plr.Character.HumanoidRootPart
        mouse = plr:GetMouse()
        mouse.KeyDown:connect(function(key)
        if key == 'c' then
        if mouse.Target then
        if clicktpon then

local args = {
    [1] = CFrame.new(mouse.Hit.x, mouse.Hit.y + 0.1, mouse.Hit.z)
}

game:GetService("ReplicatedStorage"):WaitForChild("Witches"):WaitForChild("EvilWitches"):WaitForChild("Enchantress"):WaitForChild("PowerEvents"):WaitForChild("lightningStrikeEvent"):FireServer(unpack(args))

end
end
end
end)
end)

BennettSection:NewButton("Levitation", " ", function()

local args = {
    [1] = "prepare",
    [2] = Vector3.new(-42.67594909667969, 76.22956848144531, -191.8939208984375)
}

game:GetService("ReplicatedStorage"):WaitForChild("Witches"):WaitForChild("GoodWitches"):WaitForChild("Phoebe"):WaitForChild("PowerEvents"):WaitForChild("PhoebeEvents"):FireServer(unpack(args))

end)

BennettSection:NewToggle("Kick", "Keybind (V)", function()

    clicktpon = true                
        plr = game.Players.LocalPlayer
        hum = plr.Character.HumanoidRootPart
        mouse = plr:GetMouse()
        mouse.KeyDown:connect(function(key)
        if key == 'v' then
        if mouse.Target then
        if clicktpon then

local args = {
    [1] = "attack",
    [2] = Vector3.new(mouse.Hit.x, mouse.Hit.y + 0.1, mouse.Hit.z),
    [3] = workspace:WaitForChild("Part")
}

game:GetService("ReplicatedStorage"):WaitForChild("Witches"):WaitForChild("GoodWitches"):WaitForChild("Phoebe"):WaitForChild("PowerEvents"):WaitForChild("PhoebeEvents"):FireServer(unpack(args))

end
end
end
end)
end)

BennettSection:NewButton("Whirlwind", " ", function()

local args = {
    [1] = "start",
    [2] = Vector3.new(-349.08319091796875, 7369.44287109375, 957.2813720703125)
}

game:GetService("ReplicatedStorage"):WaitForChild("Demons"):WaitForChild("UpperLevel"):WaitForChild("Shax"):WaitForChild("PowerEvents"):WaitForChild("ShaxEvents"):FireServer(unpack(args))

end)

