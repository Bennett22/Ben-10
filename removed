--auto things

_G.AutoPunch = true

function AutoPunch()

while _G.AutoPunch == true do
-- Script generated by Bennett Spy - discord: bennettscripts
bennettscripts = {
    [1] = "Combat",
    [2] = 1
}

game:GetService("ReplicatedStorage").Events.SpeedEvent:FireServer(unpack(bennettscripts))

-- Script generated by Bennett Spy - discord: bennettscripts
bennettscripts = {
    [1] = "Combat",
    [2] = 2
}

game:GetService("ReplicatedStorage").Events.SpeedEvent:FireServer(unpack(bennettscripts))

wait(0.111)
end
end

BennettTab:AddToggle({
	Name = "Auto Punch (Speed)",
	Default = false,
	Callback = function(Value)
		_G.AutoPunch = Value
                AutoPunch()
	end    
})

